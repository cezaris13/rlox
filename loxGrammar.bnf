<expression> ::= literal | unary | grouping | binary

<literal> ::= NUMBER | STRING | "true" | "false" | "nil"
<grouping> ::= "(" <expression> ")"
<unary> ::= ("-" | "!") <expression>
<binary> ::= <expression> <operator> <expression>

<operator> ::= "==" | "!=" | "<" | "<=" | ">" | ">=" | "+"  | "-" | "*" | "/"
